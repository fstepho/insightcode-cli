{
  "name": "insightcode-cli",
  "version": "0.1.0",
  "description": "TypeScript code quality analyzer that runs 100% locally",
  "main": "dist/cli.js",
  "bin": {
    "insightcode": "dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsc",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "benchmark": "node scripts/benchmark.js"
  },
  "keywords": [
    "typescript",
    "code-quality",
    "analyzer",
    "cli",
    "complexity",
    "maintainability",
    "cyclomatic-complexity",
    "code-duplication",
    "static-analysis",
    "developer-tools"
  ],
  "author": "fstepho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fstepho/insightcode-cli.git"
  },
  "bugs": {
    "url": "https://github.com/fstepho/insightcode-cli/issues"
  },
  "homepage": "https://github.com/fstepho/insightcode-cli#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "fast-glob": "^3.3.3",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}